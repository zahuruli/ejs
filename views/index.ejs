<%- include("layout/header") %>
<main>
  <form action="/" method="post">
    <input type="text" placeholder="programming language" name="planguage" />
    <button type="submit">Add</button>
  </form>

  <ul>
    <% if(planguage.length>0){ %> <% for(let i=0;i< planguage.length;i++){ %>
    <li><%= planguage[i] %></li>
    <% } %> <% }else{ %>
    <p>No Data Found</p>
    <% } %>
  </ul>
</main>
<%- include("layout/footer") %>
